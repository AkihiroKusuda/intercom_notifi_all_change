{
	"manifest_version": 2,
	"author": "MasaKusu",
	"homepage_url": "http://example.com",
	"name": "all_turn",
	"description": "全部ボタン反転しちゃうヨォん",
	"version": "0.1",
	"icons": {
		"32": "32.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["script.js"]
	}],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "32.png",
		"default_title": "all_turn"
	},
	"permissions": [
		"tabs",
		"background",
		"http://*/*",
		"https://*/*"
	]
}
